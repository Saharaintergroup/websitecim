<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="indications" model="ir.ui.view">
            <field name="name">Indications</field>
            <field name="type">qweb</field>
            <field name="key">theme_cim.indications</field>
            <field name="arch" type="xml">
                <t name="Indications" t-name="theme_cim.indications">
                    <t t-call="website.layout">
                        <div id="wrap">
                            <div class="container">
                                <h1>Indications</h1>
                            </div>
                            <div class="oe_structure"/>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="indications_page" model="website.page">
            <field name="url">/indications</field>
            <field name="website_published">True</field>
            <field name="view_id" ref="indications"/>
        </record>
        <record id="menu_indications" model="website.menu">
            <field name="name">Indications</field>
            <field name="url">/indications</field>
            <field name="page_id" ref="theme_cim.indications_page"/>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">40</field>
        </record>
    </data>
</odoo>