<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="public_announcements_page" model="website.page">
            <field name="name">Public announcements</field>
            <field name="type">qweb</field>
            <field name="url">/public-announcements</field>
            <field name="website_indexed" eval="False"/>
            <field name="is_published">True</field>
            <field name="key">theme_cim.public_announcements_page</field>
            <field name="arch" type="xml">
                <t name="Public announcements" t-name="theme_cim.public_announcements_page">
                    <t t-call="website.layout">
                        <div id="wrap">
                            <section class="section_announcements pt50 pb50">
                                <div class="container">
                                    <h3 class="hr_title_horizontal mb30">Public announcements</h3>
                                    <div class="row">
                                        <div class="col-md-4" t-foreach="public_announcements" t-as="announce">
                                            <div class="box_latest_news d-flex align-items-end mt20">
                                                <div class="cover_latest_news"
                                                     style="background-image: url('/theme_cim/static/src/images/news/news-5.png');"/>
                                                <div class="content_latest_news">
                                                    <h6 class="title_latest_news" t-esc="announce.name"/>
                                                    <p t-esc="announce.description"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <t t-call="website.pager">
                                            <t t-set="_classes">mt40 justify-content-center</t>
                                    </t>
                                </div>
                            </section>
                            <div class="oe_structure"/>
                        </div>
                    </t>
                </t>
            </field>
        </record>

