<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="aboutus" model="ir.ui.view">
            <field name="name">About us</field>
            <field name="type">qweb</field>
            <field name="key">theme_cim.aboutus</field>
            <field name="arch" type="xml">
                <t name="About us" t-name="theme_cim.aboutus">
                    <t t-call="website.layout">
                        <div id="wrap">
                            <div class="container">
                                <h1>About us</h1>
                            </div>
                            <div class="oe_structure"/>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="aboutus_page" model="website.page">
            <field name="url">/aboutus</field>
            <field name="website_published">True</field>
            <field name="view_id" ref="aboutus"/>
        </record>
        <record id="menu_aboutus" model="website.menu">
            <field name="name">About us</field>
            <field name="url">/aboutus</field>
            <field name="page_id" ref="theme_cim.aboutus_page"/>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">20</field>
        </record>
    </data>
</odoo>