<?xml version="1.0" ?>
<odoo>
    <data>
        <record id="common_questions" model="ir.ui.view">
            <field name="name">Common questions</field>
            <field name="type">qweb</field>
            <field name="key">theme_cim.common_questions</field>
            <field name="arch" type="xml">
                <t name="Common questions" t-name="theme_cim.common_questions">
                    <t t-call="website.layout">
                        <div id="wrap">
                            <div class="container">
                                <h1>Common questions</h1>
                            </div>
                            <div class="oe_structure"/>
                        </div>
                    </t>
                </t>
            </field>
        </record>
        <record id="common_questions_page" model="website.page">
            <field name="url">/common-questions</field>
            <field name="website_published">True</field>
            <field name="view_id" ref="common_questions"/>
        </record>
        <record id="menu_common_questions" model="website.menu">
            <field name="name">Common questions</field>
            <field name="url">/common-questions</field>
            <field name="page_id" ref="theme_cim.common_questions_page"/>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="sequence" type="int">90</field>
        </record>
    </data>
</odoo>